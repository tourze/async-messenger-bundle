{
  "name": "tourze/async-messenger-bundle",
  "description": "Symfony Messenger asynchronous message handling bundle with Doctrine and Redis transport implementations, failover support, and circuit breaker functionality",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "ext-SPL": "*",
    "ext-date": "*",
    "ext-filter": "*",
    "ext-json": "*",
    "ext-random": "*",
    "ext-redis": "*",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/persistence": "^4.1",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^7.3",
    "symfony/console": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/messenger": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/service-contracts": "^3.6",
    "symfony/yaml": "^7.3",
    "tourze/async-contracts": "0.0.*",
    "tourze/bundle-dependency": "1.*",
    "tourze/enum-extra": "1.*",
    "tourze/redis-dedicated-connection-bundle": "1.0.*",
    "tourze/symfony-dependency-service-loader": "1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/phpunit-bridge": "^7.3",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-enum": "1.*",
    "tourze/phpunit-symfony-kernel-test": "1.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\AsyncMessengerBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\AsyncMessengerBundle\\Tests\\": "tests"
    }
  }
}